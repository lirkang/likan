{
  "Arrow Function": {
    "prefix": "ar",
    "body": ["($1) => {$2}"]
  },
  "Async Arrow Function": {
    "prefix": "sar",
    "body": ["async ($1) => {$2};"]
  },
  "Function": {
    "prefix": "fun",
    "body": ["function ${1:name}($2) {\n\t$3\n};"]
  },
  "Async Function": {
    "prefix": "sfn",
    "body": ["async function ${1:name}($2) {\n\t$3\n};"]
  },
  "Tab": {
    "prefix": "rt",
    "body": "<${1|div,span,template|} ${2:property}></$1>"
  },
  "Single Tab": {
    "prefix": "rst",
    "body": ["<${1|div,span,template|} ${2:property} />"]
  },
  "Function Component": {
    "prefix": "rfc",
    "body": [
      "import React from 'react';\n",
      "const ${1/(.*)/${1:/capitalize}/} = () => {",
      "\treturn <$2></>;",
      "};\n",
      "export default ${1/(.*)/${1:/capitalize}/};\n"
    ]
  },
  "export default": {
    "prefix": "efa",
    "body": "export default $1;"
  },
  "useState": {
    "prefix": "uss",
    "body": ["const [${1}, set${1/(.*)/${1:/capitalize}/}] = useState($2);"]
  },
  "console": {
    "prefix": "clg",
    "body": ["console.log($1);"]
  },
  "import": {
    "prefix": "imp",
    "body": ["import ${2:module} from '${1:package}';"]
  },
  "require": {
    "prefix": "req",
    "body": ["const ${2:module} = require('${1:package}');"]
  },
  "vue3 template": {
    "prefix": "vt",
    "body": [
      "<template>",
      "\t<${6:div} $7></${6:div}>",
      "</template>\n",
      "<script lang=\"${2|ts,js|}\" setup></script>\n",
      "<script lang=\"${2|ts,js|}\">",
      "export default { name: '${3:ComponentName}' }",
      "</script>\n",
      "<style lang=\"${4|css,less,scss,sass|}\" ${5:scoped}>",
      "</style>"
    ]
  }
}
