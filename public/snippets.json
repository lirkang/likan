{
  "Arrow Function": {
    "prefix": "ar",
    "body": ["($1) => {$2}"],
    "description": "Arrow Function Snippets"
  },
  "Async Arrow Function": {
    "prefix": "sar",
    "body": ["async ($1) => {$2};"],
    "description": "Async Arrow Function Snippets"
  },
  "Function": {
    "prefix": "fun",
    "body": ["function ${1:name}($2) {\n\t$3\n};"],
    "description": "Function Snippets"
  },
  "Async Function": {
    "prefix": "sfn",
    "body": ["async function ${1:name}($2) {\n\t$3\n};"],
    "description": "Async Function Snippets"
  },
  "Tab": {
    "prefix": "rt",
    "body": "<$1></>",
    "description": "Tab Snippets"
  },
  "Single Tab": {
    "prefix": "rst",
    "body": ["<$1 />"],
    "description": "Single Tab Snippets"
  },
  "Function Component": {
    "prefix": "rfc",
    "body": [
      "import React from 'react';\n",
      "const ${1/(.*)/${1:/capitalize}/} = () => {",
      "\treturn <$2></>;",
      "};\n",
      "export default ${1/(.*)/${1:/capitalize}/};\n"
    ],
    "description": "React Function Component Snippets"
  },
  "export default": {
    "prefix": "efa",
    "body": "export default $1;",
    "description": "Default Export Snippets"
  },
  "useState": {
    "prefix": "uss",
    "body": ["const [${1}, set${1/(.*)/${1:/capitalize}/}] = useState($2);"],
    "description": "UseState Snippets"
  },
  "console": {
    "prefix": "clg",
    "body": ["console.${2|log,error,info,warn,trace,debug,table,groupCollapsed,groupEnd,group|}($1);"],
    "description": "Console Snippets"
  },
  "import": {
    "prefix": "imp",
    "body": ["import ${2:module} from '${1:package}';"],
    "description": "Import Snippets"
  },
  "require": {
    "prefix": "req",
    "body": ["const ${2:module} = require('${1:package}');"],
    "description": "CommonJS Require Snippets"
  },
  "vue3 template": {
    "prefix": "vt",
    "body": [
      "<template>",
      "\t<${4:div}></div>",
      "</template>\n",
      "<script lang=\"${2|ts,js|}\" setup></script>\n",
      "<script lang=\"${2|ts,js|}\">",
      "export default { name: '${3:ComponentName}' }",
      "</script>\n"
    ],
    "description": "Vue3 Template Snippets"
  },
  "doc-comment": {
    "prefix": "cmt",
    "body": [
      "/**",
      " * @Author likan",
      " * @Date $CURRENT_YEAR/$CURRENT_MONTH/$CURRENT_DATE $CURRENT_HOUR:$CURRENT_MINUTE:$CURRENT_SECOND",
      " * @FilePath $TM_FILEPATH",
      " */\n\n$1"
    ],
    "description": "Doc-comment Snippets"
  }
}
