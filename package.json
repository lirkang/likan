{
  "name": "Likan",
  "displayName": "Likan",
  "description": "这是一个对 vscode 所缺少的功能补全的插件...",
  "publisher": "Likan",
  "version": "0.8.83",
  "author": {
    "name": "likan"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lirkang/likan"
  },
  "galleryBanner": {
    "color": "#424243",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "main": "./lib/index.js",
  "license": "SEE LICENSE IN LICENSE",
  "icon": "public/images/likan.png",
  "keywords": [],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:likan.other.scriptsRunner",
    "onCommand:likan.open.terminal",
    "onCommand:likan.open.browser",
    "onCommand:likan.language.wrap",
    "onCommand:likan.language.comment",
    "onCommand:likan.open.currentWindow",
    "onCommand:likan.open.newWindow"
  ],
  "contributes": {
    "commands": [
      {
        "command": "likan.other.scriptsRunner",
        "title": "执行命令",
        "category": "likan"
      },
      {
        "command": "likan.open.terminal",
        "title": "切换显示终端",
        "icon": "$(terminal)",
        "category": "likan"
      },
      {
        "command": "likan.open.browser",
        "title": "在默认浏览器打开",
        "icon": "$(globe)",
        "category": "likan"
      },
      {
        "command": "likan.open.currentWindow",
        "title": "在新窗口中打开",
        "category": "likan"
      },
      {
        "command": "likan.open.newWindow",
        "title": "在当前窗口中打开",
        "category": "likan"
      },
      {
        "command": "likan.language.wrap",
        "title": "插入标签",
        "category": "likan"
      },
      {
        "command": "likan.language.comment",
        "title": "在文件头生成注释",
        "category": "likan"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "likan-explorer",
          "name": "Explorer"
        },
        {
          "id": "likan-scripts",
          "name": "Scripts"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "likan.open.newWindow",
          "group": "navigation@1",
          "when": "view =~ /explorer/"
        },
        {
          "command": "likan.open.currentWindow",
          "group": "navigation@0",
          "when": "view =~ /explorer/"
        }
      ],
      "editor/title": [
        {
          "command": "likan.open.browser",
          "group": "navigation@1",
          "when": "resourceLangId == html || resourceLangId == htm"
        },
        {
          "command": "likan.open.terminal",
          "group": "navigation@0",
          "when": "config.likan.show.terminal"
        }
      ],
      "explorer/context": [
        {
          "command": "likan.open.browser",
          "group": "navigation@1",
          "when": "resourceLangId == html || resourceLangId == htm"
        },
        {
          "command": "likan.open.currentWindow",
          "group": "navigation@1",
          "when": "!explorerResourceIsRoot"
        }
      ],
      "editor/context": [
        {
          "command": "likan.open.browser",
          "group": "navigation@1",
          "when": "resourceLangId == html || resourceLangId == htm"
        }
      ],
      "commandPalette": [
        {
          "command": "likan.open.terminal",
          "when": "false"
        },
        {
          "command": "likan.other.scriptsRunner",
          "when": "false"
        },
        {
          "command": "likan.open.currentWindow",
          "when": "false"
        },
        {
          "command": "likan.open.newWindow",
          "when": "false"
        },
        {
          "command": "likan.language.wrap",
          "when": "resourceLangId == html || resourceLangId == htm || resourceLangId == javascriptreact || resourceLangId == typescriptreact || resourceLangId == vue || resourceLangId == xml || resourceLangId == svg || resourceLangId == javascript || resourceLangId == typescript"
        },
        {
          "command": "likan.language.comment",
          "when": "resourceLangId == javascript || resourceLangId == typescript || resourceLangId == javascriptreact || resourceLangId == typescriptreact"
        },
        {
          "command": "likan.open.browser",
          "when": "resourceLangId == html || resourceLangId == htm"
        }
      ]
    },
    "languages": [
      {
        "id": "javascriptreact",
        "aliases": [
          "JavaScript React",
          "jsx"
        ],
        "extensions": [
          ".jsx"
        ],
        "configuration": "./public/configurations/javascript-language-configuration.json"
      },
      {
        "id": "javascript",
        "aliases": [
          "JavaScript",
          "javascript",
          "js"
        ],
        "extensions": [
          ".js",
          ".es6",
          ".mjs",
          ".cjs",
          ".pac"
        ],
        "filenames": [
          "jakefile"
        ],
        "firstLine": "^#!.*\\bnode",
        "mimetypes": [
          "text/javascript"
        ],
        "configuration": "./public/configurations/javascript-language-configuration.json"
      },
      {
        "id": "jsx-tags",
        "aliases": [],
        "configuration": "./public/configurations/tags-language-configuration.json"
      },
      {
        "id": "typescript",
        "aliases": [
          "TypeScript",
          "ts",
          "typescript"
        ],
        "extensions": [
          ".ts",
          ".cts",
          ".mts"
        ],
        "configuration": "./public/configurations/language-configuration.json"
      },
      {
        "id": "typescriptreact",
        "aliases": [
          "TypeScript React",
          "tsx"
        ],
        "extensions": [
          ".tsx"
        ],
        "configuration": "./public/configurations/language-configuration.json"
      },
      {
        "id": "jsonc",
        "filenames": [
          "tsconfig.json",
          "jsconfig.json"
        ],
        "filenamePatterns": [
          "tsconfig.*.json",
          "jsconfig.*.json",
          "tsconfig-*.json",
          "jsconfig-*.json"
        ]
      },
      {
        "id": "json",
        "filenames": [
          "tsconfig.tsbuildinfo"
        ]
      }
    ],
    "keybindings": [
      {
        "key": "alt+shift+w",
        "command": "likan.language.wrap",
        "when": "editorFocus"
      },
      {
        "key": "alt+shift+x",
        "command": "likan.language.comment",
        "when": "editorFocus"
      }
    ],
    "configuration": {
      "title": "likan",
      "properties": {
        "likan.enum.manager": {
          "type": "string",
          "enum": [
            "npm",
            "yarn",
            "pnpm"
          ],
          "default": "npm",
          "description": "包管理工具"
        },
        "likan.string.author": {
          "type": "string",
          "description": "插入注释时显示的Author字段值",
          "default": "likan"
        },
        "likan.show.terminal": {
          "type": "boolean",
          "description": "是否在标题栏显示Terminal",
          "default": true
        },
        "likan.show.fileSize": {
          "type": "boolean",
          "description": "是否在状态栏显示文件大小",
          "default": true
        },
        "likan.show.memory": {
          "type": "boolean",
          "description": "是否在状态栏显示内存占用",
          "default": true
        },
        "likan.list.tags": {
          "type": "array",
          "description": "插入标签时展示的候选标签",
          "default": [
            "div",
            "span",
            "template"
          ]
        },
        "likan.list.exts": {
          "type": "array",
          "description": "查询跳转时自动检索后缀名",
          "default": [
            ".js",
            ".ts",
            ".jsx",
            ".tsx"
          ]
        },
        "likan.list.alias": {
          "type": "object",
          "description": "路径跳转的别名映射, ${root}代表工作区根目录",
          "default": {
            "@": "${root}/src",
            "~": "${root}"
          }
        },
        "likan.list.folders": {
          "type": "array",
          "description": "explorer需要检索的目录"
        },
        "likan.list.filterFolders": {
          "type": "array",
          "description": "explorer中过滤的关键字",
          "default": [
            "node_modules",
            ".vscode",
            ".git"
          ]
        }
      }
    },
    "snippets": [
      {
        "language": "javascript",
        "path": "./public/configurations/snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./public/configurations/snippets.json"
      },
      {
        "language": "typescript",
        "path": "./public/configurations/snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./public/configurations/snippets.json"
      },
      {
        "language": "vue",
        "path": "./public/configurations/snippets.json"
      },
      {
        "language": "html",
        "path": "./public/configurations/snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "run-s package",
    "package": "cross-env NODE_ENV=production webpack",
    "develop": "cross-env NODE_ENV=development webpack --watch",
    "preview": "cross-env NODE_ENV=test webpack --profile --progress",
    "release": "standard-version -a -r patch",
    "commit": "cz",
    "format": "prettier ./*.js ./src/**/* -w -u --cache",
    "fix": "run-s format lint",
    "lint": "eslint ./src/**/* --fix --quiet --cache --cache-location ./node_modules/.cache/eslint/.eslintcache --cache-strategy content",
    "precommit": "run-s fix"
  },
  "devDependencies": {
    "@types/vscode": "^1.66.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
    "ts-loader": "^9.2.8",
    "typescript": "~4.7",
    "webpack": "^5.70.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "open": "^8.4.0"
  },
  "standard-version": {
    "scripts": {
      "precommit": "git add .",
      "prerelease": "npm run fix"
    }
  },
  "volta": {
    "node": "16.17.0",
    "yarn": "1.22.19",
    "npm": "8.15.0"
  }
}
