{
  "name": "Likan",
  "displayName": "Likan",
  "description": "一个插件",
  "publisher": "likan",
  "version": "0.6.13",
  "repository": "https://github.com/LIiErkang/likan",
  "engines": {
    "vscode": "^1.66.0"
  },
  "icon": "./public/likan.png",
  "categories": [
    "Other",
    "Snippets",
    "Keymaps"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "likan.npmStart",
        "title": "Run Script",
        "category": "likan",
        "enablement": "true"
      },
      {
        "command": "likan.npmSelect",
        "title": "Scripts Selection",
        "category": "likan",
        "enablement": "true"
      },
      {
        "command": "likan.terminalToggle",
        "title": "Switch Terminal",
        "category": "likan",
        "enablement": "true"
      },
      {
        "command": "likan.openInBrowser",
        "title": "Open In Default Browser",
        "icon": "$(globe)",
        "category": "likan",
        "enablement": "resourceLangId == html || resourceLangId == htm"
      },
      {
        "command": "likan.htmlWrap",
        "title": "Insert HTML Tabs",
        "category": "likan",
        "enablement": "resourceLangId == html || resourceLangId == htm || resourceLangId == javascriptreact || resourceLangId == typescriptreact || resourceLangId == vue"
      },
      {
        "command": "likan.insertComment",
        "title": "Insert Doc-Comment",
        "category": "likan",
        "enablement": "resourceLangId == javascript || resourceLangId == typescript || resourceLangId == javascriptreact || resourceLangId == typescriptreact"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "likan.openInBrowser",
          "group": "navigation",
          "when": "resourceLangId == html || resourceLangId == htm"
        }
      ],
      "explorer/context": [
        {
          "command": "likan.openInBrowser",
          "group": "navigation",
          "when": "resourceLangId == html || resourceLangId == htm"
        }
      ]
    },
    "keybindings": [
      {
        "command": "likan.htmlWrap",
        "key": "alt+shift+w"
      },
      {
        "key": "ctrl+alt+a",
        "command": "cursorLeft"
      },
      {
        "key": "ctrl+alt+s",
        "command": "cursorDown"
      },
      {
        "key": "ctrl+alt+d",
        "command": "cursorRight"
      },
      {
        "key": "ctrl+alt+w",
        "command": "cursorUp"
      },
      {
        "key": "alt+shift+x",
        "command": "likan.insertComment"
      }
    ],
    "configuration": {
      "title": "likan",
      "properties": {
        "likan.npmStart": {
          "type": "string",
          "default": "start",
          "description": "执行的npm命令(不要加 npm run 前缀)"
        },
        "likan.packManager": {
          "type": "string",
          "enum": [
            "npm",
            "yarn"
          ],
          "default": "npm",
          "description": "包管理工具"
        }
      }
    },
    "snippets": [
      {
        "language": "javascript",
        "path": "./public/snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./public/snippets.json"
      },
      {
        "language": "typescript",
        "path": "./public/snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./public/snippets.json"
      },
      {
        "language": "vue",
        "path": "./public/snippets.json"
      },
      {
        "language": "html",
        "path": "./public/snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "ts-loader": "^9.2.8",
    "typescript": "^4.5.5",
    "vscode": "^1.1.37",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {}
}
