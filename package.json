{
  "name": "Likan",
  "displayName": "Likan",
  "description": "一个插件",
  "publisher": "likan",
  "version": "0.7.11",
  "repository": "https://github.com/LIiErkang/likan",
  "engines": {
    "vscode": "^1.66.0"
  },
  "browser": "./lib/index.js",
  "main": "./lib/index.js",
  "license": "MIT",
  "icon": "public/icon.png",
  "categories": [
    "Other",
    "Snippets"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "likan.npm.select",
        "title": "选择命令运行",
        "category": "likan"
      },
      {
        "command": "likan.language.openInBrowser",
        "title": "在默认浏览器打开",
        "icon": "$(globe)",
        "category": "likan",
        "enablement": "resourceLangId == html || resourceLangId == htm"
      },
      {
        "command": "likan.language.htmlWrap",
        "title": "插入标签",
        "category": "likan",
        "enablement": "resourceLangId == html || resourceLangId == htm || resourceLangId == javascriptreact || resourceLangId == typescriptreact || resourceLangId == vue"
      },
      {
        "command": "likan.language.insertComment",
        "title": "在文件头生成注释",
        "category": "likan",
        "enablement": "resourceLangId == javascript || resourceLangId == typescript || resourceLangId == javascriptreact || resourceLangId == typescriptreact"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "likan.language.openInBrowser",
          "group": "navigation@1",
          "when": "resourceLangId == html || resourceLangId == htm"
        }
      ],
      "explorer/context": [
        {
          "command": "likan.language.openInBrowser",
          "group": "navigation@1",
          "when": "resourceLangId == html || resourceLangId == htm"
        }
      ],
      "editor/context": [
        {
          "command": "likan.npm.select",
          "group": "navigation@0"
        },
        {
          "command": "likan.language.openInBrowser",
          "group": "navigation@1",
          "when": "resourceLangId == html || resourceLangId == htm"
        }
      ]
    },
    "keybindings": [
      {
        "key": "alt+shift+w",
        "command": "likan.language.htmlWrap",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+alt+a",
        "command": "cursorLeft",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+alt+s",
        "command": "cursorDown",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+alt+d",
        "command": "cursorRight",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+alt+w",
        "command": "cursorUp",
        "when": "editorFocus"
      },
      {
        "key": "alt+shift+x",
        "command": "likan.language.insertComment",
        "when": "editorFocus"
      }
    ],
    "configuration": {
      "title": "likan",
      "properties": {
        "likan.npm.manager": {
          "type": "string",
          "enum": [
            "npm",
            "yarn",
            "pnpm"
          ],
          "default": "npm",
          "description": "包管理工具"
        },
        "likan.language.author": {
          "type": "string",
          "description": "插入注释时显示的Author字段值",
          "default": "likan"
        },
        "likan.statusbar.fileSize": {
          "type": "boolean",
          "description": "是否在状态栏显示文件大小",
          "default": true
        },
        "likan.statusbar.memory": {
          "type": "boolean",
          "description": "是否在状态栏显示内存占用",
          "default": true
        },
        "likan.statusbar.terminal": {
          "type": "boolean",
          "description": "是否在状态栏显示Terminal",
          "default": true
        },
        "likan.language.htmlTag": {
          "type": "array",
          "description": "插入标签时展示的候选标签",
          "default": [
            "div",
            "span",
            "template"
          ]
        },
        "likan.language.exts": {
          "type": "array",
          "description": "查询跳转时自动检索后缀名",
          "default": [
            ".js",
            ".ts",
            ".jsx",
            ".tsx"
          ]
        }
      }
    },
    "snippets": [
      {
        "language": "javascript",
        "path": "./public/snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./public/snippets.json"
      },
      {
        "language": "typescript",
        "path": "./public/snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./public/snippets.json"
      },
      {
        "language": "vue",
        "path": "./public/snippets.json"
      },
      {
        "language": "html",
        "path": "./public/snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "package": "webpack --mode production",
    "watch": "webpack --watch --devtool inline-source-map",
    "pub": "ts-node ./scripts/publish.ts 没有日志"
  },
  "devDependencies": {
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vscode": "^1.1.37",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {}
}