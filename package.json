{
  "name": "Likan",
  "displayName": "Likan",
  "description": "一个插件",
  "publisher": "likan",
  "version": "0.6.25",
  "repository": "https://github.com/LIiErkang/likan",
  "engines": {
    "vscode": "^1.66.0"
  },
  "browser": "./lib/index.js",
  "main": "./lib/index.js",
  "license": "MIT",
  "icon": "public/icon.png",
  "categories": [
    "Other",
    "Snippets",
    "Keymaps"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "likan.npmStart",
        "title": "Run Script",
        "icon": "$(run)",
        "category": "likan"
      },
      {
        "command": "likan.npmSelect",
        "title": "Scripts Selection",
        "category": "likan"
      },
      {
        "command": "likan.terminalToggle",
        "title": "Switch Terminal",
        "category": "likan"
      },
      {
        "command": "likan.openInBrowser",
        "title": "Open In Default Browser",
        "icon": "$(globe)",
        "category": "likan",
        "enablement": "resourceLangId == html || resourceLangId == htm"
      },
      {
        "command": "likan.htmlWrap",
        "title": "Insert HTML Tabs",
        "category": "likan",
        "enablement": "resourceLangId == html || resourceLangId == htm || resourceLangId == javascriptreact || resourceLangId == typescriptreact || resourceLangId == vue"
      },
      {
        "command": "likan.insertComment",
        "title": "Insert Doc-Comment",
        "category": "likan",
        "enablement": "resourceLangId == javascript || resourceLangId == typescript || resourceLangId == javascriptreact || resourceLangId == typescriptreact"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "likan.openInBrowser",
          "group": "navigation@1",
          "when": "resourceLangId == html || resourceLangId == htm"
        },
        {
          "command": "likan.npmStart",
          "group": "navigation@0"
        }
      ],
      "explorer/context": [
        {
          "command": "likan.openInBrowser",
          "group": "navigation@1",
          "when": "resourceLangId == html || resourceLangId == htm"
        }
      ],
      "editor/context": [
        {
          "command": "likan.npmSelect",
          "group": "navigation@0"
        },
        {
          "command": "likan.openInBrowser",
          "group": "navigation@1",
          "when": "resourceLangId == html || resourceLangId == htm"
        }
      ]
    },
    "keybindings": [
      {
        "key": "alt+shift+w",
        "command": "likan.htmlWrap",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+alt+a",
        "command": "cursorLeft",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+alt+s",
        "command": "cursorDown",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+alt+d",
        "command": "cursorRight",
        "when": "editorFocus"
      },
      {
        "key": "ctrl+alt+w",
        "command": "cursorUp",
        "when": "editorFocus"
      },
      {
        "key": "alt+shift+x",
        "command": "likan.insertComment",
        "when": "editorFocus"
      }
    ],
    "configuration": {
      "title": "likan",
      "properties": {
        "likan.npmStart": {
          "type": "string",
          "default": "start",
          "description": "需要执行的命令"
        },
        "likan.packManager": {
          "type": "string",
          "enum": [
            "npm",
            "yarn"
          ],
          "default": "npm",
          "description": "包管理工具"
        }
      }
    },
    "snippets": [
      {
        "language": "javascript",
        "path": "./public/snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./public/snippets.json"
      },
      {
        "language": "typescript",
        "path": "./public/snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./public/snippets.json"
      },
      {
        "language": "vue",
        "path": "./public/snippets.json"
      },
      {
        "language": "html",
        "path": "./public/snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "package": "webpack --mode production",
    "watch": "webpack --watch --devtool inline-source-map",
    "lint": "eslint src --ext ts",
    "pub": "node ./dist/scripts/publish.js 没有日志"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "ts-loader": "^9.2.8",
    "typescript": "^4.5.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vscode": "^1.1.37",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {}
}