{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}" /* , "--disable-extensions" */],
      "outFiles": ["${workspaceFolder}/lib/**/*.js"],
      "preLaunchTask": "${defaultBuildTask}",
    }
  ]
}
